<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="family" class="com.tj.method1.Family"
		c:papaName="홍아빠" c:mamiName="김엄마"
		p:brotherName="홍아들" p:sisterName="홍딸램" />
	<bean name="st3" class="com.tj.method1.Student">
		<constructor-arg value="홍길동"/>
		<constructor-arg value="22"/>
		<constructor-arg>
			<list>
				<value>수영</value><value>독서</value>
			</list>
		</constructor-arg>
		<property name="weight" value="70"/>
		<property name="height" value="170"/>
	</bean>
</beans>








